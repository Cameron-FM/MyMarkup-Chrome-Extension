{
  "manifest_version": 2,
  "name": "My Markup",
  "homepage_url": "http://localhost:8080/",
  "description": "A Chrome Extension to help you save you HTML page edits",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "*://*/*",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/MyMarkup Small Logo.png",
    "48": "icons/MyMarkup Small Logo.png",
    "128": "icons/MyMarkup Small Logo.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "My Markup",
    "default_icon": {
      "19": "icons/MyMarkup Small Logo.png",
      "38": "icons/MyMarkup Small Logo.png"
    }
  }
}